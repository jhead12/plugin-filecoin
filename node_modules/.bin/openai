#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/bin/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/bin/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules:/Users/jeldonmusic/Documents/matrixblend_com/eliza/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/openai@4.91.0_encoding@0.1.13_ws@8.18.0_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.24.2/node_modules/openai/bin/cli" "$@"
fi
